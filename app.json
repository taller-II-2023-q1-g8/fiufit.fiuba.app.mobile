{
  "expo": {
    "name": "fiuFit",
    "slug": "fiuFit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app.png",
    "userInterfaceStyle": "dark",
    "androidNavigationBar": {
      "backgroundColor": "#000000"
    },
    "splash": {
      "image": "./src/assets/app.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "permissions": ["com.google.android.gms.permission.ACTIVITY_RECOGNITION", "android.permission.ACTIVITY_RECOGNITION"],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app.png",
        "backgroundColor": "#000000"
      },
      "package": "com.santiagobiach.fiuFit"
    },
    "plugins": [
      ["@react-native-google-signin/google-signin"],
      ["expo-image-picker"],
      ["expo-local-authentication"],
      ["@react-native-firebase/app"],
      ["expo-location"],
      ["expo-task-manager"],
      ["expo-sensors"]
    ],
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9829a76e-1c34-4b71-86a4-e821a2181927"
      }
    }
  }
}